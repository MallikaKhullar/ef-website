            }
            .grid-container {
                padding: 0px 20px 10px 20px;
            }
            .blog-outside-title {
                overflow: hidden;
                text-overflow: ellipsis;
                -webkit-line-clamp: 2;
                display: -webkit-box;
                -webkit-box-orient: vertical;
                font-family: Roboto;
                font-size: 22px;
                font-weight: bold;
                line-height: 28px;
                margin: 10px 0px;
            }
            .blog-short-desc {
                overflow: hidden;
                text-overflow: ellipsis;
                -webkit-line-clamp: 2;
                display: -webkit-box;
                -webkit-box-orient: vertical;
                color: #4C4C4C;
                font-family: "Source Serif Pro";
                font-size: 17px;
                line-height: 22px;
                margin: 7px 0px;
                height: 45px;
            }
            .grid-column {
                float: left;
                width: 100%;
                padding: 0px 10px 0px 10px;
                cursor: pointer;
            }
            .grid-column img {
                margin-top: 50px;
                border-radius: 5px;
            }
            .big-title {
                color: #000000;
                font-family: Quicksand;
                font-size: 35px;
                font-weight: bold;
                line-height: 30px;
                margin: 30px 0px -20px 10px;
            }
            .category-container {
                display: none;
            }
            .category-container-small {
                display: block;
                width: 100%;
                padding: 15px;
            }
            #blog-header {
                display: none;
            }
            #blog-header-small {
                display: block;
                padding: 20px 0px;
                overflow: auto;
            }
        }
        
        @media screen and (min-width: 700px) {
            .paginate-small {
                display: none;
            }
            .paginate-big {
                display: block;
            }
            .category-container {
                margin: 90px 0px 10px 0px;
                padding: 10px 130px 0px 126px;
                width: 100%;
                height: 80px;
            }
            .category-container-small {
                display: none;
            }
            #blog-header {
                display: block;
            }
            #blog-header-small {
                display: none;
            }
        }
        
        .blog-short-desc {
            margin: 0px 0px 15px 0px;
        }
        
        .blog-outside-title {
            margin: 10px 0px 5px 0px;
        }
        
        .more-pink-title {
            color: #EF2A67;
            font-family: Quicksand;
            font-size: 14px;
            font-weight: bold;
            line-height: 18px;
        }
        
        .center-align {
            text-align: center;
        }
        
        .small-margin-top {
            margin-top: 10px;
        }
        
        .big-margin-top {
            margin-top: 60px;
        }
        
        .small-margin-bottom {
            margin-bottom: 10px;
        }
        
        .footer-title {
            font-size: 16px;
            font-weight: bold;
            line-height: 20px;
            margin-bottom: 20px;
        }
        
        .footer-desc {
            display: none
        }
        
        .footer-icons {
            font-size: 20px;
        }
        
